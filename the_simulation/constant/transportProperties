FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              0.08;
    rho             915;
}

air
{
    transportModel  Newtonian;
    nu              1.49;
    rho             1260;
}

sigma            0.0352;

